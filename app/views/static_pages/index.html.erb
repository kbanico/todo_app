<section class="todoapp">
    <header class="header">
      <h1>todos</h1>
    </header>

    <section class="main">
      <ul class="todo-list">

      </ul>

    </section>

</section>

<script>
  $(function(){
    $.get("/tasks").success(function(data){
      data.forEach(function(element){
        console.log(element.title)
        $(".todo-list").append("<li>"+element.title+"</li>")
      })

    })
  });
</script>
